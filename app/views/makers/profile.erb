<%=@maker.username%>'s peeps:

<% @peeps.reverse! %>
<% @peeps.each do |peep| %>
<div><a href='/peeps/filter/<%=peep.maker.username%>'><%=peep.maker.username%></a> peeped:</div>
<div><%=peep.text%></div>
<div>on: <%=peep.time%></div>
<% end %>


<form action='/peeps'>
  <input type='submit' value='see all peeps'>
</form>
